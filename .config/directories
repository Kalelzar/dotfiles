cache ${XDG_CACHE_HOME:-$HOME/.cache}
pcache ${XDG_PERSIST_HOME:-$HOME/.persist}
conf  ${XDG_CONFIG_HOME:-$HOME/.config}
downl   ${XDG_DOWNLOAD_DIR:-$HOME/Downloads}
doc  ${XDG_DOCUMENTS_DIR:-$HOME/Documents}
data  ${XDG_DATA_HOME:-$HOME/.local/share}
runtime  ${XDG_RUNTIME_HOME:-$HOME/.persist/1000}
h   $HOME
music   ${XDG_MUSIC_DIR:-$HOME/Music}
mnt  /mnt
scr  $HOME/Code/Scripts
src $HOME/.local/src
